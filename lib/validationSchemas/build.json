{
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "pattern": "^(success|failed)$"
    },
    "date": {
      "type": "string",
      "pattern": "[0-9]{4,}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(?:\\.[0-9]+|)(?:[+-][0-9]{2}:[0-9]{2}|Z)"
    },
    "revision": {
      "type": "string"
    },
    "details": {
      "$ref": "details"
    }
  },
  "required": [
    "name",
    "status",
    "date"
  ],
  "additionalProperties": false
}