{
  "extends": "eslint:recommended",
  "env": {
    "node": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "objectLiteralShorthandProperties": true,
    "destructuring": true,
    "modules": true,
    "templateStrings": true
  },
  "rules": {
    "curly": 2,
    "semi": 2,
    "eqeqeq": 2,
    "no-unused-expressions": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-console": 0
  },
  "globals": {
    "jasmine": false,
    "beforeAll": false,
    "afterAll": false,
    "beforeEach": false,
    "afterEach": false,
    "describe": false,
    "fdescribe": false,
    "xdescribe": false,
    "it": false,
    "xit": false,
    "fit": false,
    "pending": false,
    "expect": false,
    "fail": false,
    "by": false,
    "element": false,
    "browser": false,
    "describeUseCase": false,
    "describeScenario": false
  }
}
