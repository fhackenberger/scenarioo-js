{
  "extends": "eslint:recommended",
  "env": {
    "node": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "objectLiteralShorthandProperties": true,
    "destructuring": true,
    "modules": true,
    "templateStrings": true
  },
  "rules": {
    "curly": 2,
    "semi": 2,
    "eqeqeq": 2,
    "no-unused-expressions": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-console": 0
  },
  "globals": {
    "browser": false,
    "jasmine": false,
    "describe": false,
    "fdescribe": false,
    "it": false,
    "by": false,
    "fit": false,
    "element": false,
    "afterEach": false,
    "beforeEach": false,
    "before": false,
    "after": false
  }
}
